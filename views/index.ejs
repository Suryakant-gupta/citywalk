<script>
    // Wait for the DOM to be fully loaded
    document.addEventListener('DOMContentLoaded', function() {
        // Get the button element
        const readMoreBtn = document.getElementById('read-more-btn');
        
        // Get all hidden features
        const hiddenFeatures = document.querySelectorAll('.hidden-feature');
        
        // Add click event listener to the button
        if (readMoreBtn) {
            readMoreBtn.addEventListener('click', function() {
                // Check current button text
                if (this.textContent === 'Read More') {
                    // Show all hidden features
                    hiddenFeatures.forEach(feature => {
                        feature.classList.add('show');
                    });
                    // Change button text
                    this.textContent = 'Read Less';
                } else {
                    // Hide all features
                    hiddenFeatures.forEach(feature => {
                        feature.classList.remove('show');
                    });
                    // Change button text back
                    this.textContent = 'Read More';
                    // Scroll back to the top of the feature list
                    document.querySelector('.feature-list').scrollIntoView({behavior: 'smooth'});
                }
            });
        }
    });
    </script>

    <script>
        document.addEventListener('DOMContentLoaded', function() {
    const overviewReadMoreBtn = document.getElementById('overview-read-more-btn');
    const overviewHiddenText = document.getElementById('overview-hidden-text');

    if (overviewReadMoreBtn && overviewHiddenText) {
        overviewReadMoreBtn.addEventListener('click', function() {
            if (overviewHiddenText.classList.contains('show')) {
                overviewHiddenText.classList.remove('show');
                overviewReadMoreBtn.textContent = 'Read More';
            } else {
                overviewHiddenText.classList.add('show');
                overviewReadMoreBtn.textContent = 'Read Less';
            }
        });
    }
});

